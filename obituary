    <h1 class="headline entry-title">Arch Perry -- directed public works projects</h1>
    <!-- e src/business/templates/hearst/article/headline.tpl -->
									                <!-- hearst/common/author_name.tpl -->
<p class="byline">Matthai Chakko Kuruvila, Chronicle Staff Writer</p>
<!-- e hearst/common/author_name.tpl -->
		<h5 class="timestamp" title="2007-01-22T04:00:00Z">
			Published 4:00&nbsp;am, Monday, January 22, 2007
		</h5>
	</div>
<!-- e ux hearst/article/types/standard_title.tpl -->
