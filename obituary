<script>
// SSO-597
$(document).ready(function() {

	var htmlcode = 
	  '<div id="systemUpdateOverlay"></div>'
	+ '<div id="systemUpdateOverlayContainer">'
	+ '	<div id="systemUpdateOverlayContent">'
	+ '		<div class="header">'
	+ '			<div class="left"><span>System Update</span></div>'
	+ '			<div class="right"><span class="close">X</span></div>'
	+ '		</div>'
	+ '		<div class="body">'
	+ '			<p>A recent registration system update means that all current site users and subscribers need to reset their passwords when logging in to SFGATE or SFChronicle.com for the first time, starting Tuesday, September 13, 2016.</p>'
	+ '			<p><b>To reset your password:</b></p>'
	+ '			<ul>'
	+ '				<li><span>1</span><h5>Click on "forgot password?" link on the page where you are signing in</h5></li>'
	+ '				<li><span>2</span><h5>Follow the instructions to reset your password</h5></li>'
	+ '				<li><span>3</span><h5>Sign in and you are done!</h5></li>'
	+ '			</ul>'
	+ '			<p>Questions? Call our customer care hotline at 800-310-2455 (hours below), or via email at homedelivery@sfchronicle.com.</p>'
	+ '			<ul class="hours">'
	+ '				<li><h5>Monday - Friday: 6:30am - 4:30pm PST</h5></li>'
	+ '				<li><h5>Saturdays & Holidays: 6:30am - 10:30am PST</h5></li>'
	+ '				<li><h5>Sundays: 8:00am - 11:00am PST</h5></li>'
	+ '			</ul>'
	+ '		</div>'
	+ '		<div class="button">'
	+ '			<span>Ok</span>'
	+ '		</div>'
	+ '	</div>'
	+ '</div>';
	$("body").prepend(htmlcode);

	function displayModal() {
		var height = "calc((100% - " + $("#systemUpdateOverlayContainer").height() + "px) / 2)";
		$("#systemUpdateOverlayContainer").css("top", height);

		$("#systemUpdateOverlay").show();
		$("#systemUpdateOverlayContainer").show();

		setTimeout(function() {
			$("#systemUpdateOverlay").off().on("click touchstart", function(e) {
				hideModal();
				$("#systemUpdateOverlay").off();
			});
			$("#systemUpdateOverlayContent, #systemUpdateOverlayContent .header .close, #systemUpdateOverlayContent .button").off().on("click touchstart", function(e) {
				e.stopPropagation();
				e.preventDefault();
				hideModal();
				$("#systemUpdateOverlayContent, #systemUpdateOverlayContent .header .close, #systemUpdateOverlayContent .button").off();
			});
			$(document).keydown(function(e) {
				if(e.keyCode == 27) {
					hideModal();
				}
			});
		}, 500);
	}

	function hideModal() {
		$("#systemUpdateOverlay").hide();
		$("#systemUpdateOverlayContainer").hide();
	}

	function initModal() {
		if(!localStorage.getItem("firstVisit")) {
			localStorage.setItem("firstVisit", "false");
			displayModal();
		}
	}

	initModal();

});
</script>
  
                      
  
