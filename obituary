        if (!elem || !elem.id || !elem.id.indexOf || elem.id.indexOf('vembaDynamic') == -1) return;
        var parts = elem.id.split('vembaDynamic');
        if (parts.length !== 2) return;
        var zoneID = parts[1];
        if (Object.keys(window.vembaLoadedList).length) return;
        if (window.vembaLoadedList[zoneID]) return;
        window.vembaLoadedList[zoneID] = 1;
 
 
        var script = document.createElement('script');
        script.type = 'text/javascript';
        script.src = 'http://videos-by.vemba.io/v2/placements/'+zoneID+'.js';
