    <div id="ctpl-fullbanner1" class="ctpl-fullbanner" data-collapse-timer="10">
    <div id="A728" class="ad_deferrable"><script type="text/javascript">/*<![CDATA[*/ hearstPlaceAd("A728"); /*]]>*/</script></div>
    
    <script>
    (function() {
        var checkCount = 120;
        var intervalID = setInterval(function() {
            if(checkCount > 0) {
                checkCount--;
                setTimeout(function() {
                    if(document.getElementById("A728").offsetHeight > 3) {
                        if(!document.documentElement.classList.contains("header-ad") && !document.documentElement.classList.contains("cropped")) {
                            document.documentElement.className += " header-ad header-A728 header-adSlide";
                            setTimeout(function() {
                                document.documentElement.classList.remove("header-adSlide");
                            }, 500);
                            clearInterval(intervalID);
                        }
                    }
                }, 2500);
            }
            else {
                if(document.getElementById("A728").offsetHeight <= 3) {
                    document.documentElement.className += " header-noAd";
                }
                clearInterval(intervalID);
            }
        }, 1000);
    })();
    </script>
    
